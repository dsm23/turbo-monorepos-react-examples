{
  "name": "@repo/react-ui-compiled",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup",
    "clean": "rm -rf dist",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "1.2.4",
    "@repo/common-utils": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/stylelint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "4.1.17",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.22",
    "class-variance-authority": "0.7.1",
    "jest": "30.2.0",
    "postcss": "8.5.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwindcss": "4.1.17",
    "tsup": "8.5.1",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  }
}
