{
  "name": "@repo/react-ui-compiled",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup",
    "clean": "rm -rf dist",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "1.2.0",
    "@repo/common-utils": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/stylelint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "4.1.5",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "jest": "29.7.0",
    "postcss": "8.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "4.1.5",
    "tsup": "8.4.0",
    "tw-animate-css": "1.2.9",
    "typescript": "5.8.3"
  }
}
