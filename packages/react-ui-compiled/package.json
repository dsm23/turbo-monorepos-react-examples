{
  "name": "@repo/react-ui-compiled",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup",
    "clean": "rm -rf dist",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css && tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,mjs,ts,tsx}\" --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@radix-ui/react-slot": "1.2.3",
    "@repo/common-utils": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/stylelint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/cli": "4.1.12",
    "@types/jest": "30.0.0",
    "@types/node": "24.2.1",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "jest": "30.0.5",
    "postcss": "8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwindcss": "4.1.12",
    "tsup": "8.5.0",
    "tw-animate-css": "1.3.6",
    "typescript": "5.9.2"
  }
}
